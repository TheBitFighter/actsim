/*

    Test basic functionality of sequence multi source with logging off and looping off

*/

import sim;
import globals;

defproc test ()
{
    pint tokens = 2;
    pint data[tokens];
    data = {3, 9};

    sim::source_sequence_multi<8, 2, tokens, data, false, 0, false> src;
    sim::sink<8, 0, true> snk0;
    sim::sink<8, 1, true> snk1;

    snk0.I = src.O[0];
    snk1.I = src.O[1];

    src.enable = Vdd;
    snk0.enable = Vdd;
    snk1.enable = Vdd;
}
