deftype struct (int<2> a, b) { }

defproc src(chan(struct)! X)
{
  struct x;
  int<2> tmp;
  int count;
  chp {
     count := 0;
     x.a := 0;
     x.b := 1;
     *[ count < 1000 -> X!x; tmp:=x.a; x.a := x.b; x.b := tmp; count := count+1 ]
  }
}

defproc sink(chan(struct)? X)
{
  struct y;
  chp {
   *[ X?y; log("got: ", y.a, " ", y.b) ]
  }
  dataflow {
    X -> *
  }
}

defproc test()
{
  src s;
  sink t(s.X); 
}

test t;
