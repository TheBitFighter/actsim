deftype struct (int<4> a, b) { }

function newstruct (struct s) : struct 
{
  int i;
  chp {
    i := 5;
   *[ i > 0 -> i := i - 1 ];
    self.a := s.b;
    self.b := s.a
  }
}

function addup (struct s) : int<5>
{
  chp {
    self := s.a + s.b
  }
}

defproc test()
{
  struct s;
  int t;
  chp {
    s.a := 5;
    s.b := 10;
    t := 0;
    t := addup(s);
    log ("Sum is: ", t);
    s := newstruct(s);
    log ("a is: ", s.a, " b is: ", s.b)
  }
}
