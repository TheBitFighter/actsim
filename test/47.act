import std;
open std::channel;

defproc src (e1of2 x)
{
  int i;
  chp {
     i := 0;
    *[ i < 100 -> x!(i & 1); i := i + 1; log("sent!") ]
  }
}

defproc sink (e1of2 x)
{
  bool ax, bx;
  prs {
    x.t => ax+
    x.f => bx+
    ax | bx => x.e-
  }
}

defproc test ()
{
   src s;
   sink t(s.x);
}

